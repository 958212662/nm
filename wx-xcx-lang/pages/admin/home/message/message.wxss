Page {
   background-color: #eee;
}

.container {
  display: flex;
  flex-direction: column;
}

.item {
  height: 220rpx;
  font-size: 14px;
  border-bottom: 1rpx solid #B5B5B5;
  padding-left: 20rpx;
  padding-top: 20rpx;
  background-color: white;
}

.item-body {
  display: flex;
  flex-direction: column;
  width: 95%;
  height: 180rpx;
  justify-content: space-between;
  color: #B5B5B5;
}

.title {
  display: flex;
  font-size: 16px;
  color: black;
}

.title image{
  width: 48rpx;
  height: 48rpx;
}

.title text{
  margin-left: 20rpx;
  width: 90%;
}

.foot {
  display: flex;
  align-items: center;
  justify-content: space-between;

}

.foot image{
  width: 48rpx;
  height: 48rpx;
}

.nodata {
  width: 100%;
  height: 100%;
  background-color: #FFFFFF;
}

.nodata image{
  width: 750rpx;
  height: 1260rpx;
  margin-left: 50rpx;
  margin-top: 100rpx;
  
}

.more {
  width: 100%;
  height: 68rpx;

  color: #aaa;
  font-size: 14px;
  line-height: 68rpx;
  text-align: center;
}
