Page {
  background-color: #EEEEEE
}

.container_body {
  display: flex;
  flex-direction: column;
  margin-top: 20rpx; 

}

.container_item {
  display: flex;
  height: 200rpx;
  background-color: white;
  align-items: center;
  border-bottom: 1rpx solid #cccccc;
  border-left: 1rpx solid #cccccc;
  border-right: 1rpx solid #cccccc;
}

.left {
  margin-left: 20rpx;
}

.left image{
  width: 160rpx;
  height: 160rpx;
}

.right {
  margin-left: 30rpx;
  height: 160rpx;
  width:70%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.right_headle{
  font-size: 16px;
  width: 98%;
  display: flex;
  justify-content: space-between;
}

.right_body{
  display: flex;
  justify-content: space-between;
  width: 80%;
  color: #999999;
  font-size: 22rpx;
}

.breed {
  font-size: 15px;
  color: #9D9D9D;
}

.ownerName {
  color:black;
  font-size: 16px;
  font-weight: 500;
}

.right_foot {
  color:black;
  font-size: 14px;
  font-weight: 500;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.more {
  width: 100%;
  height: 68rpx;
  position: flexd;
  bottom: 0;
  color: #aaaaaa;
  font-size: 14px;
  line-height: 68rpx;
  text-align: center;
}

.nodata {
  width: 100%;
  height: 100%;
  background-color: #FFFFFF;
}

.nodata image{
  width: 750rpx;
  height: 1260rpx;
  margin-left: 50rpx;
  margin-top: 100rpx;
  
}

.add_dog {
  /*子绝父相*/
  
}

.add_dog::after{
  /*通过伪元素设置一像素的右边框*/
  content: "新增犬只";
  /*定位，不占位置*/
  position: fixed;
  width: 120rpx;
  height: 120rpx;
  text-align: center;
  right: 10rpx;
  bottom: 10rpx;
  font-size: 14px;
  background-color: #ff9a68;
  color: white;
  border-radius: 60rpx;
  box-sizing: border-box;
  padding: 25rpx 30rpx;

}

/* 功能封装 防疫登记 */
.epidemic-register{
  display: flex;
  justify-content: space-between;
}

.epidemic-register::after {
  content: "防疫登记";
  width: 140rpx;
  height: 60rpx;
  display: block;
  font-size: 14px;
  text-align: center;
  line-height: 60rpx;
  background-color: #66CCFF;
  color: white;
  border-radius: 10rpx;
}

