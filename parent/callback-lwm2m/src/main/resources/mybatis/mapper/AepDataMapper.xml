<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soholy.mapper.AepDataMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.soholy.entity.AepData">
        <id column="seq_id" property="seqId" />
        <result column="content" property="content" />
        <result column="cretion_time" property="cretionTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        seq_id, content, cretion_time
    </sql>

</mapper>
