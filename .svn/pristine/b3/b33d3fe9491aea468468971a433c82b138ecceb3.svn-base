package com.posiition.VO;

import com.entities.OrgInfo;

import java.io.Serializable;
import java.util.List;

public class Org implements Serializable {

    /**
     * 机构id
     */
    private Integer orgid;

    /**
     * 机构列表
     */
    private List<OrgInfo> orgInfos;


    /**
     * 是否是机构 默认不是
     */
    private boolean isOrg;

    public Integer getOrgid() {
        return orgid;
    }

    public void setOrgid(Integer orgid) {
        this.orgid = orgid;
    }

    public List<OrgInfo> getOrgInfos() {
        return orgInfos;
    }

    public void setOrgInfos(List<OrgInfo> orgInfos) {
        this.orgInfos = orgInfos;
    }

    public Org() {
    }

    public boolean isOrg() {
        return isOrg;
    }

    public void setOrg(boolean org) {
        isOrg = org;
    }

    public Org(Integer orgid, List<OrgInfo> orgInfos) {
        this.orgid = orgid;
        this.orgInfos = orgInfos;
    }
}
