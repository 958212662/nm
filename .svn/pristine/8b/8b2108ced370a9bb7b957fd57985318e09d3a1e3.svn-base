<body oncontextmenu=self.event.returnValue=false onselectstart="return false"> 
<!--wrap-->
<div class="wrap">

<!--内容栏-->
<div class="section2">
	<!--面包屑-->
	<div class="tit pdl0">
		<ul class="ul03 ovh" style="margin-left: 3px;">
			<li class="fl"><a href="###"th:text="#{orgPower}">组织权限></a></li>
			<li class="fl"><a href="###"th:text="#{userManage}">用户管理></a></li>
			<li class="fl"th:text="#{dogManage.edit2}">修改用户</li>
		</ul>
	</div>
	<!--面包屑END-->
	
	<!--内容区-->
	<div class="pdt20 pdb75">
		<form id="userEditForm" action="#">
			<div class="pdl0 pdr50">
				<div class="ovh mgt10">
					<div class="fl w70 cor3 h50 lh50 t-r" th:text="'*'+#{userManage.area}">*所属区域</div>
					<div class="mgl90">
						<input id="areaSelect" select="selected" th:value="${user.areaId}" name="areaId" class="ww30 h50 lh50 bore ras4 pdl30 cor9 pc22">
					</div>
				</div>
				
				<!--<div class="ovh mgt10">-->
					<!--<div class="fl w70 cor3 h50 lh50 t-r">*用户编号</div>-->
					<!--<div class="mgl90">-->
						<!--<input type="text" class="ww92 h50 lh50 bore ras4 pdl30" placeholder="">-->
					<!--</div>-->
				<!--</div>-->

				<div class="ovh mgt10">
					<div class="fl w70 cor3 h50 lh50 t-r"th:text="'*'+#{userManage.name}">*姓名</div>
					<div class="mgl90">
						<input name="name" th:value="${user.name}" type="text" class="ww92 h50 lh50 bore ras4 pdl30" placeholder="">
					</div>
				</div>

				<div class="ovh mgt10">
					<div class="fl w70 cor3 h50 lh50 t-r" th:text="'*'+#{userManage.account}">*用户名称</div>
					<div class="mgl90">
						<input name="username"  readonly="readonly" th:value="${user.username}"  type="text" class="ww92 h50 lh50 bore ras4 pdl30" placeholder="">
					</div>
				</div>

				<!--<div class="ovh mgt10">-->
					<!--<div class="fl w70 cor3 h50 lh50 t-r">*密码</div>-->
					<!--<div class="mgl90">-->
						<!--<input name="password" th:value="${user.password}"  type="text" class="ww92 h50 lh50 bore ras4 pdl30" placeholder="">-->
					<!--</div>-->
				<!--</div>-->
				
				<div class="ovh mgt10">
					<div class="fl w70 cor3 h50 lh50 t-r"th:text="'*'+#{dogManage.phone}">*联系方式</div>
					<div class="mgl90">
						<input name="mobile"  th:value="${user.mobile}"  type="text" class="ww92 h50 lh50 bore ras4 pdl30" placeholder="">
					</div>
				</div>

				<div class="ovh mgt10">
					<div class="fl w70 cor3 h50 lh50 t-r"th:text="'*'+#{userManage.org}">*所属组织</div>
					<div class="mgl90">
						<input id="orgSelect"  name="deptId" th:value="${user.deptId}" class="ww30 h50 lh50 bore ras4 pdl30 cor9 pc22">
					</div>
				</div>

				<div class="ovh mgt10">
					<div class="fl w70 cor3 h50 lh50 t-r" th:text="'*'+#{userManage.status}">*状态</div>
					<div class="mgl90 ovh pdt17">
						<ul class="fl ovh">
							<li  class="fl mgr50">
								<label class="lab1" >
									<input class="dinb" name="status" 	th:field="*{user.status}" value="1" type="radio"/>
									<i style="margin-right: 10px;" class="dinb" ></i><span  th:text="#{userManage.normal}"></span>
								</label>
							</li>
							<li  class="fl mgr50">
								<label class="lab1" >
									<input class="dinb" name="status" 	th:field="*{user.status}"  value="0" type="radio"/>
									<i style="margin-right: 10px;" class="dinb" ></i><span  th:text="#{userManage.disabled}"></span>
								</label>
							</li>
						</ul>
					</div>
				</div>
				<input type="hidden" name="roleIds" th:value="${user.roleIds}" id="roleIds">
				<input type="hidden" th:value="${user.userId}" name="userId" id="userId">
				<div class="ovh mgt10">
					<div class="fl w70 cor3 h50 lh50 t-r"th:text="'*'+#{userManage.role}">*所属角色</div>
					<div class="mgl90 ovh pdt17">
						<ul class="fl ovh">
							<li  th:each="role,index:${roles}" class="fl mgr50">
								<label class="lab1" >
									<input class="dinb" name="role" onclick="checkRoleNum();" type="checkbox" th:value="${role.roleId}" th:checked="${role.roleSign}"/>
									<i style="margin-right: 10px;" class="dinb" ></i>
									<p class="dinb"  th:utext="${role.roleName}"></p>
								</label>
							</li>
						</ul>
					</div>
				</div>
				
				<!--<div class="ovh mgt10">
					<div class="fl w70 cor3 h50 lh50 t-r">*视频</div>
					<div class="mgl90 ovh">
						<div class="file rel">
							<input type="file">
							<img src="images/44.png"> 
						</div>
					</div>
				</div>
				
				<div class="ovh mgt10">
					<div class="fl w70 cor3 h50 lh50 t-r">描述</div>
					<div class="mgl90">
						<textarea class="ww92 h140 bore pdl30 pdt10 pdb10 pdr30" placeholder="10年"></textarea>
					</div>
				</div>-->
				
				<div id="btnss" class="ovh mgt85">
					<div class="mgl90">
						<input class="a6 dinb bort hov8" type="button" onclick="update(this.form)" th:value="#{dogManage.save}">
						<a href="###" class="a6 dinb hov8" onclick="javascript:window.history.back(-1);"th:text="#{dogManage.cancel}">取消</a>
					</div>
				</div>
				
			</div>
		</form>
	</div>
	<!--内容区END-->
	
</div>
<!--内容栏END-->
	
</div>
<!--wrap End-->
<div th:include="include::footer"></div>
</body>
</html>
<script type="text/javascript" src="/js/easyui/js/jquery.easyui.min.js"></script>
