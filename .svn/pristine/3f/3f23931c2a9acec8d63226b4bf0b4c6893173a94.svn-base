<!doctype html>
<html>
<meta charset="utf-8">
<head  th:include="include :: header">
<meta http-equiv="Content-Type" content="multipart/form-data; charset=utf-8" />
<meta http-equiv="x-ua-compatible" content="ie=9"> 
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

<title>A19-犬只更换记录</title>
<meta name="description" content="">
<meta name="keywords" content="">

<!--[if lt IE 9]>
<script src="https:// oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js">
</script>
<script src="https:// oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js">
</script>
<![endif]-->
</head>
<body oncontextmenu=self.event.returnValue=false onselectstart="return false"> 
<!--wrap-->
<div class="wrap">

<!--内容栏-->
<div class="section2">
	<!--面包屑-->
	<div class="tit pdl50">
		<ul class="ul03 ovh">
			<li class="fl"><a href="###"th:text="#{antiepidemicManage}">防疫管理></a></li>
			<li class="fl"><a href="/biz/infectionInfo/toInfectionInfo_List"th:text="#{antiepidemicManage.infectionInfo}">牛羊感染及病变脏器处理></a></li>
			<li class="fl"th:text="#{dogManage.edit2}">修改</li>
		</ul>
	</div>
	<!--面包屑END-->
	
	<!--内容区-->
	<div class="pdt20 pdb75">
		<form action="#" id="infectionUpdate">
			<input th:value="${detail.id}" name="id" type="text" hidden="hidden">
			<div class="pdl30 pdr30">
				<div class="ovh mgt10">
					<div class="fl w100 cor3 h50 lh50 t-r"th:text="#{antiepidemicManage.surveyNum2}">*调查数</div>
					<div class="mgl120">
						<input type="text" class="ww92 h50 lh50 bore ras4 pdl30" 
						id="detectionAmount" name="detectionAmount" th:value="${detail.detectionAmount}" onblur="checkDetection()" >
					</div>
				</div>
				<div class="ovh mgt10">
					<div class="fl w100 cor3 h50 lh50 t-r"th:text="'*'+#{antiepidemicManage.infectionNum}">*感染数</div>
					<div class="mgl120">
						<input id="infectionAmount" name="infectionAmount" th:value="${detail.infectionAmount}" onblur="checkInfection()" class="ww92 h50 lh50 bore ras4 pdl30">
					</div>
				</div>
				
				<div class="ovh mgt10">
					<div class="fl w100 cor3 h50 lh50 t-r"th:text="#{antiepidemicManage.harmlessTreatNum2}">*无害化处理数</div>
					<div class="mgl120">
						<input  name="harmlessTreatAmount"  th:value="${detail.harmlessTreatAmount}" class="ww92 h50 lh50 bore ras4 pdl30">
					</div>
				</div>  
				<div class="ovh mgt10">
					<div class="fl w100 cor3 h50 lh50 t-r"th:text="#{antiepidemicManage.detectionTime}">*检测时间</div>
					<div class="mgl120">
						<input  readonly="readonly" th:value="${#dates.format(detail.detectionDate, 'yyyy-MM-dd HH:mm:ss')}" type="text" class="ww92 h50 lh50 bore ras4 pdl30 pc23">
					</div>
				</div> 
				<div class="ovh mgt40">
					<div class="mgl120">
						<input class="a6 dinb bort hov8" type="button" onclick="updateInfectionInfo(this.form)" th:value="#{dogManage.save}">
						<a href="/biz/infectionInfo/toInfectionInfo_List" class="a6 dinb hov8"th:text="#{dogManage.cancel}">取消</a>
					</div>
				</div>
				
			</div>
		</form>
	</div>
	<!--内容区END-->
	
</div>
<!--内容栏END-->
	
</div>
<div th:include="include::footer"></div>
<script src="/epmanagement/infectionInfo/InfectionInfo_Update.js"></script>
<!--wrap End-->
</body>
</html>
<script th:inline="javascript">
    var pleaseInputSurveyNum=[[#{antiepidemicManage.pleaseInputSurveyNum}]];
    var pleaseInputInfectionNum=[[#{antiepidemicManage.pleaseInputInfectionNum}]];
    var pleaseInputHarmlessNum=[[#{antiepidemicManage.pleaseInputHarmlessNum}]];
    var pleaseInputLegal=[[#{antiepidemicManage.pleaseInputLegal}]];
    var pleaseSelectPhoto=[[#{dogManage.pleaseSelectPhoto}]];
    var unKnownError=[[#{antiepidemicManage.unKnownError}]];
    var operationSuccess=[[#{operationSuccess}]];
    var operationFailure=[[#{operationFailure}]];
    var sure5=[[#{dogManage.sure}]];
</script>