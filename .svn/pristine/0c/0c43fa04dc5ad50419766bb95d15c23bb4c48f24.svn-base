.xz_element{
  /*通过伪元素设置一像素的右边框*/
  content: "新增";
  /*定位，不占位置*/
  position: fixed;
  width: 120rpx;
  height: 120rpx;
  text-align: center;
  right: 10rpx;
  bottom: 10rpx;
  font-size: 14px;
  background-color: #ff9a68;
  color: white;
  border-radius: 60rpx;
  box-sizing: border-box;
  padding: 25rpx 20rpx;
  line-height: 30rpx;

 white-space: pre-wrap;
  display: flex;
  justify-content: center;
  align-items: center;
}


