Page {
  background-color: #EEEEEE
}

.container_body {
  display: flex;
  flex-direction: column;
  margin-top: 20rpx; 

}

.container_item {
  display: flex;
  height: 200rpx;
  background-color: white;
  align-items: center;
  border-bottom: 1rpx solid #cccccc;
  border-left: 1rpx solid #cccccc;
  border-right: 1rpx solid #cccccc;
}

.body {
  width: 82%;
  height: 160rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-left: 40rpx;
}

.title {
  display: flex;
  font-size: 16px;
  color: #999999;
}

.left {
  width: 150rpx;
}

.middle {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 14px;
  color: #999999
}

.middle view{
  width: 50%;
}

.middle text{
  color: black;
}

.foot {
  display: flex;
  font-size: 14px;
  color: #999999;
  justify-content: space-between;
}

.neckletNo{
  font-size: 13px;
  color: black;
}

.more {
  width: 100%;
  height: 68rpx;
  position: flexd;
  bottom: 0;
  color: #aaaaaa;
  font-size: 14px;
  line-height: 68rpx;
  text-align: center;
}



/* .necklet-change::after */
.xqgh_element{
  /*通过伪元素设置一像素的右边框*/
  content: "项圈更换";
  /*定位，不占位置*/
  position: fixed;
  width: 120rpx;
  height: 120rpx;
  text-align: center;
  right: 10rpx;
  bottom: 10rpx;
  font-size: 14px;
  background-color: #ff9a68;
  color: white;
  border-radius: 60rpx;
  box-sizing: border-box;
  padding: 25rpx 20rpx;
  line-height: 30rpx;
  white-space: pre-wrap;
  display: flex;
  justify-content: center;
  align-items: center;
}